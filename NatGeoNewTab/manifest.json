{

  "manifest_version": 2,
  "name": "Nat Geo New Tab",
  "version": "1.0",

  "description": "Displays NatGeo photograph of the day on opening a new tab",

  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },


  "permissions": [
    "tabs"
  ],

  "chrome_url_overrides": {
    "newtab": "background.html"
  }
}
